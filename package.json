    "url": "git+https://github.com/takefumi-yoshii/unistore-immer.git"
  },
  "author": "takefumi-yoshii",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/takefumi-yoshii/unistore-immer/issues"
  },
  "homepage": "https://github.com/takefumi-yoshii/unistore-immer#readme",
  "babel": {
